@{
}

@model List<MonthlyClaimMaster.Models.Claim>

<table>
    <thead>
        <tr>
            <th>Claim ID</th>
            <th>Lecturer ID</th>
            <th>Date Submitted</th>
            <th>Total Payment</th>
            <th>Status</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var claim in Model)
        {
            <tr>
                <td>@claim.ClaimID</td>
                <td>@claim.LecturerID</td>
                <td>@claim.DateSubmitted</td>
                <td>@claim.TotalPayment</td>
                <td>@claim.Status</td>
            </tr>
        }
    </tbody>
</table>

